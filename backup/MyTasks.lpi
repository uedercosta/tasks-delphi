<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <ProjectOptions>
    <Version Value="12"/>
    <PathDelim Value="\"/>
    <General>
      <SessionStorage Value="InProjectDir"/>
      <Title Value="MyTasks"/>
      <Scaled Value="True"/>
      <ResourceType Value="res"/>
      <UseXPManifest Value="True"/>
      <XPManifest>
        <DpiAware Value="True"/>
      </XPManifest>
      <Icon Value="0"/>
    </General>
    <BuildModes>
      <Item Name="Default" Default="True"/>
    </BuildModes>
    <PublishOptions>
      <Version Value="2"/>
      <UseFileFilters Value="True"/>
    </PublishOptions>
    <RunParams>
      <FormatVersion Value="2"/>
    </RunParams>
    <RequiredPackages>
      <Item>
        <PackageName Value="FCL"/>
      </Item>
      <Item>
        <PackageName Value="zcomponent"/>
      </Item>
      <Item>
        <PackageName Value="LCL"/>
      </Item>
    </RequiredPackages>
    <Units>
      <Unit>
        <Filename Value="MyTasks.lpr"/>
        <IsPartOfProject Value="True"/>
      </Unit>
      <Unit>
        <Filename Value="Views\views.menuprincipal.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="Form1"/>
        <ResourceBaseClass Value="Form"/>
        <UnitName Value="Views.MenuPrincipal"/>
      </Unit>
      <Unit>
        <Filename Value="Interfaces\interfaces.iconexao.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Interfaces.iConexao"/>
      </Unit>
      <Unit>
        <Filename Value="Models\Conexao\models.conexao.modelconexao.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Models.Conexao.ModelConexao"/>
      </Unit>
      <Unit>
        <Filename Value="Views\Cadastros\views.cadastros.tarefa.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="FrmCadTarefas"/>
        <ResourceBaseClass Value="Form"/>
        <UnitName Value="Views.Cadastros.Tarefa"/>
      </Unit>
      <Unit>
        <Filename Value="Views\Cadastros\views.cadastros.usuario.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="FrmCadUsuarios"/>
        <HasResources Value="True"/>
        <ResourceBaseClass Value="Form"/>
        <UnitName Value="Views.Cadastros.Usuario"/>
      </Unit>
      <Unit>
        <Filename Value="Models\Entidades\models.entity.usuario.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Models.Entity.Usuario"/>
      </Unit>
      <Unit>
        <Filename Value="Models\Query\models.query.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Models.Query"/>
      </Unit>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="11"/>
    <PathDelim Value="\"/>
    <Target>
      <Filename Value="MyTasks"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="$(ProjOutDir)"/>
      <OtherUnitFiles Value="Views;Interfaces;Models\Conexao;Views\Cadastros;Models\Entidades;Models\Query"/>
      <UnitOutputDirectory Value="lib\$(TargetCPU)-$(TargetOS)"/>
    </SearchPaths>
    <Linking>
      <Debugging>
        <DebugInfoType Value="dsDwarf2Set"/>
      </Debugging>
      <Options>
        <Win32>
          <GraphicApplication Value="True"/>
        </Win32>
      </Options>
    </Linking>
  </CompilerOptions>
  <Debugging>
    <Exceptions>
      <Item>
        <Name Value="EAbort"/>
      </Item>
      <Item>
        <Name Value="ECodetoolError"/>
      </Item>
      <Item>
        <Name Value="EFOpenError"/>
      </Item>
    </Exceptions>
  </Debugging>
</CONFIG>
